#include <bits/stdc++.h>
using namespace std;
int main(){
    int a[10], b[10], sum = 0;
    for (int i = 0; i < 10; ++i){
        cin >> a[i];
    }
    for (int i = 0; i < 10; ++i) {
        cin >> b[i];
    }
    for (int i = 0; i < 10; ++i) {
        a[i] = a[i] + b[i];
        if(a[i] >= 80){
            sum++;
        }
    }
    cout << sum;
}
